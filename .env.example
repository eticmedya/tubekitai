APP_NAME="TubeKit AI"
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_TIMEZONE=Europe/Istanbul
APP_URL=http://localhost

APP_LOCALE=tr
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=tr_TR

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=tubekitai
DB_USERNAME=root
DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=file
CACHE_PREFIX=tubekitai

MAIL_MAILER=smtp
MAIL_HOST=mailpit
MAIL_PORT=1025
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="noreply@tubekitai.com"
MAIL_FROM_NAME="${APP_NAME}"

# YouTube API - https://console.cloud.google.com/ adresinden API anahtarınızı alın
YOUTUBE_API_KEY=youtube-api-anahtarinizi-buraya-girin

# AI Providers - https://platform.openai.com/api-keys adresinden API anahtarınızı alın
OPENAI_API_KEY=openai-api-anahtarinizi-buraya-girin

# Anthropic (Claude) - https://console.anthropic.com/ adresinden API anahtarınızı alın
ANTHROPIC_API_KEY=anthropic-api-anahtarinizi-buraya-girin

AI_DEFAULT_PROVIDER=openai

# fal.ai (Kapak Görseli Üretimi) - https://fal.ai/dashboard/keys adresinden API anahtarınızı alın
FAL_API_KEY=fal-ai-api-anahtarinizi-buraya-girin

# PayTR Ödeme Sistemi - https://www.paytr.com/ adresinden bilgilerinizi alın
PAYTR_MERCHANT_ID=paytr-merchant-id-buraya-girin
PAYTR_MERCHANT_KEY=paytr-merchant-key-buraya-girin
PAYTR_MERCHANT_SALT=paytr-merchant-salt-buraya-girin
PAYTR_TEST_MODE=true

VITE_APP_NAME="${APP_NAME}"
